{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Non-docs docs",
  "colors": {
    "primary": "#0098EA",
    "light": "#F7F9FB",
    "dark": "#1E2337"
  },
  "fonts": {
    "family": "Inter"
  },
  "appearance": {
    "default": "system",
    "strict": false
  },
  "thumbnails": {
    "appearance": "dark"
  },
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "contextual": {
    "options": [
      "copy",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "navigation": {
    "pages": [
      "mdx/file1",
      "mdx/edit-me",
      {
        "group": "Nested",
        "pages": [
          "mdx/nested/file2"
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/novusnota/continuous-wasteland"
    }
  },
  "errors": {
    "404": {
      "redirect": false,
      "title": "Page not found",
      "description": "Maybe you were looking for one of these pages below?"
    }
  },
  "seo": {
    "metatags": {
      "canonical": "https://github.com/novusnota/continuous-wasteland"
    }
  },
  "redirects": [
    {
      "source": "/file1",
      "destination": "/mdx/file1",
      "permanent": true
    }
  ]
}
